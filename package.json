  ],
  "bugs": {
    "url": "https://github.com/lxjwlt/divide-webpack-plugin/issues"
  },
  "engines": {
    "node": ">= 6.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lxjwlt/divide-webpack-plugin.git"
  },
  "homepage": "https://github.com/lxjwlt/divide-webpack-plugin",
  "devDependencies": {
    "chai": "^4.0.1",
    "css-loader": "^0.28.4",
    "dir-compare": "^1.4.0",
    "eslint": "^4.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.4.2",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "zombie": "^5.0.5"
